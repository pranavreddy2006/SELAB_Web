pipeline {
    agent any
    tools{
        maven 'MAVEN-HOME'
    }
    stages {
        stage('git repo & clean') {
            steps {
                //bat "rmdir  /s /q mavenjava"
                bat "git clone provide your github link"
                bat "mvn clean -f mavenjava"
            }
        }
        stage('install') {
            steps {
                bat "mvn install -f mavenjava" #project name#
            }
        }
        stage('test') {
            steps {
                bat "mvn test -f mavenjava"
            }
        }
        stage('package') {
            steps {
                bat "mvn package -f mavenjava"
            }
        }
    }
}


or


pipeline {
    agent any

    tools {
        maven 'MAVEN-HOME'   // Make sure this name is configured in Jenkins
    }

    stages {
        stage('Checkout Repo') {
            steps {
                git url: 'https://github.com/pranavreddy2006/SELAB_Java.git'
            }
        }

        stage('Clean') {
            steps {
                bat "mvn clean -f pom.xml"
            }
        }

        stage('Install') {
            steps {
                bat "mvn install -f pom.xml"
            }
        }

        stage('Test') {
            steps {
                bat "mvn test -f pom.xml"
            }
        }

        stage('Package') {
            steps {
                bat 'mvn package -f pom.xml'
            }
        }
    }
}
